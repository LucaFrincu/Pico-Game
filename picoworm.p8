pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
x, y = 64, 64
ax, ay = 0, 0
vx, vy = 0, 0

hist_x = {}
hist_y = {}


function _draw()
	cls()
	for i=1,10 do
		circ(hist_x[i],hist_y[i],i+2,i)
	end
	print(ax..' '..ay)
	print(vx..' '..vy)
end

function _update()
	ay = 0
	if (btn(⬆️)) ay=-5 
	if (btn(⬇️)) ay=5
	if (btn(⬅️)) ax=-3
	if (btn(➡️)) ax=3
	
	if ax > 0 then ax = ax-0.1 end
	if ax < 0 then ax = ax+0.1 end
	ay = ay + 0.9 --gravity
	
	vx = vx + ax * 0.1
	vy = vy + ay * 0.1
	

--	if vx < 3 then vx = 3 end
--if vx > -3 then vx = -3 end
--if vy < 3 then vy = 3 end
--	if vy > -3 then vy = -3 end
	
	x = x + vx
	y = y + vy
	
	x = x % 128
	if y > 120 then 
		y = 120
		vy = 0 
	end
	
	add(hist_x, x)
	add(hist_y, y)
	if (#hist_x > 10) then
		deli(hist_x, 1)
		deli(hist_y, 1)
	end
end
